{
    "name": "@jayalfredprufrock/mongoes",
    "private": false,
    "version": "0.5.1",
    "description": "",
    "author": "Andrew Smiley <jayalfredprufrock@gmail.com>",
    "license": "MIT",
    "main": "./dist/mongoes.cjs.js",
    "types": "./dist/mongoes.cjs.d.ts",
    "exports": {
        ".": {
            "require": {
                "types": "./dist/mongoes.d.ts",
                "default": "./dist/mongoes.cjs.js"
            },
            "import": {
                "types": "./dist/mongoes.d.ts",
                "default": "./dist/mongoes.es.js"
            }
        },
        "./sift": {
            "require": {
                "types": "./dist/sift.d.ts",
                "default": "./dist/sift.cjs.js"
            },
            "import": {
                "types": "./dist/sift.d.ts",
                "default": "./dist/sift.es.js"
            }
        },
        "./util": {
            "require": {
                "types": "./dist/util.d.ts",
                "default": "./dist/util.cjs.js"
            },
            "import": {
                "types": "./dist/util.d.ts",
                "default": "./dist/util.es.js"
            }
        }
    },
    "files": [
        "src",
        "dist"
    ],
    "scripts": {
        "build": "vite build",
        "lint": "eslint \"src/**/*.ts\" --fix",
        "prepare": "husky",
        "test": "vitest run --coverage"
    },
    "lint-staged": {
        "./{src,test}/**/*.ts": [
            "eslint --fix"
        ]
    },
    "devDependencies": {
        "@changesets/cli": "^2.24.4",
        "@types/node": "^20.12.7",
        "@typescript-eslint/eslint-plugin": "^7.7.1",
        "@typescript-eslint/parser": "^7.7.1",
        "@vitest/coverage-v8": "^1.5.1",
        "eslint": "^8.46.0",
        "eslint-config-prettier": "^9.0.0",
        "eslint-plugin-import": "^2.28.0",
        "eslint-plugin-prettier": "^5.0.0",
        "husky": "^9.0.11",
        "lint-staged": "^15.2.2",
        "prettier": "^3.0.1",
        "sift": "^17.1.3",
        "typescript": "^5.0.4",
        "vite": "^5.2.10",
        "vite-plugin-dts": "^3.9.0",
        "vitest": "^1.5.1"
    },
    "publishConfig": {
        "access": "public"
    },
    "keywords": [
        "mongodb",
        "elasticsearch",
        "opensearch",
        "query-bridge",
        "sift",
        "siftjs",
        "es",
        "query-convertor"
    ],
    "repository": {
        "type": "git",
        "url": "https://github.com/jayalfredprufrock/mongoes"
    },
    "peerDependencies": {
        "sift": "^17.1.3"
    },
    "peerDependenciesMeta": {
        "sift": {
            "optional": true
        }
    }
}
